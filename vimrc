colorscheme abstract
syntax on
set expandtab
set shiftwidth=4
set tabstop=4
set number
set ruler
set visualbell
set incsearch
set hlsearch
set autoread
set backspace=indent,eol,start
let g:vim_json_conceal=0
let g:markdown_syntax_conceal = 0
autocmd FileType markdown setlocal spell
filetype plugin indent on
